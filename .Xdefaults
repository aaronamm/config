
!package is aur/otf-inconsolata-powerline-git
URxvt*font:			xft:Inconsolata for Powerline:size=14  
URxvt*scrollBar:		false
URxvt*scrollBar:		false
URxvt.urgentOnBell:		true
URxvt.depth:		32
urxvt.borderless: true

Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintfull
Xft.rgba: rgb

URxvt*background:  rgba:0000/0000/0000/dddd
*foreground: #eeeeee
!black
*color0: #262729
*color8: #554444
!red
*color1: #F92672
*color9: #FF669D
!green
*color2: #A6E22E
*color10: #BEED5F
!yellow
*color3: #FD971F
*color11: #E6DB74
!blue
!*color4: #66D9EF
!*color12: #66D9EF
*color4: #0190bc
*color12: #0190bc
!magenta
*color5: #9E6FFE
*color13: #9E6FFE
!cyan
*color6: #5E7175
*color14: #A3BABF
!white
*color7: #CCCCC6
*color15: #F8F8F2

# URLs
URxvt*perl-ext:  default,url-select
URxvt*matcher.button:   1
URxvt*url-select.launcher:      google-chrome
URxvt*url-select.underline:     true
